# Variables de entorno para ModeloMrlAmisPythonService

# Puerto gRPC
GRPC_PORT=50051
MRL_AMIS_PORT=50051

# Configuración del algoritmo
MAX_ITERATIONS=100
CONVERGENCE_THRESHOLD=0.001
POPULATION_SIZE=50

# Configuración de logging
LOG_LEVEL=INFO
PYTHONUNBUFFERED=1

# Rutas de datos
DATA_DIR=/app/data
OUTPUT_DIR=/app/output
